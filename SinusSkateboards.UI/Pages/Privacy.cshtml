@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<body>
     Slideshow container 
    <div class="slideshow-container">

         Full-width images with number and caption text 
        <div class="mySlides">
            <img src="~/images/0b2e1694-0c33-4e5a-88c9-59bc3f4e3758-NEW! Sinus Hoodie.png" style="width:100%">
            <div class="text">Caption Text</div>
        </div>

        <div class="mySlides">
            <img src="~/images/049413a0-124f-4486-b9f7-4f92b1f3899a-NEW! Sinus Hoodie.png" style="width:100%">
            <div class="text">Caption Two</div>
        </div>

        <div class="mySlides">
            <img src="~/images/0fda8a22-879b-45ed-830c-bbe277c292d4-Sinus T-Shirt.png" style="width:100%">
            <div class="text">Caption Three</div>
        </div>

         Next and previous buttons 
        <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
        <a class="next" onclick="plusSlides(1)">&#10095;</a>
        <a class="next" onClick="plusSlides(1)">&#10095;</a>
    </div>
    <br>

     The dots/circles 
    <div style="text-align:center">
        <span class="dot" onclick="currentSlide(1)"></span>
        <span class="dot" onclick="currentSlide(2)"></span>
        <span class="dot" onclick="currentSlide(3)"></span>
    </div>
</body>

